{
    "version": 2,
    "headers": [
        {
          "source": "/api/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Credentials", "value": "true" },
            { "key": "Access-Control-Allow-Origin", "value": "*" },
            { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
            { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version" }
          ]
        }
      ],
    "builds": [
      {
        "src": "api/alunos.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/tutores.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/materias.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/alunos", "dest": "/api/alunos.js", "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"]},
      { "src": "/api/alunos/login", "dest": "/api/alunos.js", "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"]},
      { "src": "/api/alunos/cadastro", "dest": "/api/alunos.js", "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"]},
      { "src": "/api/alunos/:id", "dest": "/api/alunos.js", "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"]},
      { "src": "/api/tutores", "dest": "/api/tutores.js", "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"]},
      { "src": "/api/tutores/login", "dest": "/api/tutores.js", "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"] },
      { "src": "/api/tutores/cadastro", "dest": "/api/tutores.js", "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"]},
      { "src": "/api/tutores/:id", "dest": "/api/tutores.js", "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"]},
      { "src": "/api/materias", "dest": "/api/materias.js", "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"]},
      { "src": "/api/materias/:id", "dest": "/api/materias.js", "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"]}
    ]
  }
  